{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/display-name": 0
  },
  "settings": {
    "react": {
      "createClass": "createReactClass",                         
      "pragma": "React", 
      "version": "16.8.6"
    }
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "js": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ]
}